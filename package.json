{
  "name": "byte-email",
  "version": "1.0.0",
  "description": "Byte AI Email Handler - Email interface for Byte AI assistant",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "start": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "keywords": [
    "byte",
    "ai",
    "email",
    "assistant"
  ],
  "author": "Chris Bergstrom",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@upstash/redis": "^1.34.3",
    "hono": "^4.11.6",
    "openai": "^6.18.0",
    "pino": "^10.3.0",
    "resend": "^4.0.1",
    "svix": "^1.42.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "bun-types": "^1.3.7",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.53.1"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
